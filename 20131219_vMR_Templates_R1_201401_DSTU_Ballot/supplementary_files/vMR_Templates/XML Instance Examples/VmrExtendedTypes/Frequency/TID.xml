 xsi:schemaLocation="urn:hl7-org:vmr:r2 ../../../../schemata/vmr/vmr.xsd" xmlns="urn:hl7-org:vmr:r2">
    <cycleLength value="24" unit="h"/>
    <cycleTiming xsi:type='RecurringEvent'>
        <!-- Before meals -->
        <when code="123" codeSystem="1.111.11111.11.1.1" codeSystemName="SNOMED-CT"/>
